<configuration>

  <connectionStrings>
   <add name="amaralguinchoConnectionString" connectionString="server=127.0.0.1;user id=root;password=password;persistsecurityinfo=False;database=amaralguincho"
      providerName="MySql.Data.MySqlClient" />
  </connectionStrings>

  <system.web>
    <sessionState timeout="43200"></sessionState>
    <globalization
      fileEncoding="utf-8"
      requestEncoding="utf-8"
      responseEncoding="utf-8"
      culture="en-US"
      uiCulture="de-DE"
    />
    <customErrors mode="RemoteOnly">
      <error statusCode="404" redirect="~/app/404.aspx"/>
    </customErrors>
    <compilation debug="true" targetFramework="4.6.1"/>
    <httpRuntime targetFramework="4.6.1" />
  </system.web>

</configuration>
